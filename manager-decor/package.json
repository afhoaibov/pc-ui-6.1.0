{
  "name": "manager-decor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"node ./ui-watcher.js\" \"vue-cli-service serve\" --kill-others",
    "serve": "yarn upgrade ui-components ui-domain ui-utils && vue-cli-service serve",
    "build": "yarn upgrade ui-components ui-domain ui-utils && vue-cli-service build",
    "build:test": "yarn upgrade ui-components ui-domain ui-utils && vue-cli-service build --mode test",
    "build:prod": "yarn upgrade ui-components ui-domain ui-utils && vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@itbuilder/auto-tester-helper": "^1.0.0",
    "@wangeditor/editor": "^5.1.21",
    "@wangeditor/editor-for-vue": "^1.0.2",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.4",
    "element-ui": "^2.15.12",
    "jquery": "^3.6.0",
    "js-base64": "^3.6.0",
    "js-cookie": "^2.2.1",
    "js-md5": "^0.7.3",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.21",
    "psl": "^1.8.0",
    "remixicon": "^2.5.0",
    "swiper": "5.x",
    "ui-components": "file:../ui-components",
    "ui-domain": "file:../ui-domain",
    "ui-utils": "file:../ui-utils",
    "uuid": "^8.3.2",
    "vant": "^2.12.12",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-i18n": "^8.25.0",
    "vue-router": "3.5.4",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.27.7",
    "@babel/eslint-parser": "7.27.5",
    "@types/node": "^22.5.4",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "babel-eslint": "^10.1.0",
    "big-integer": "^1.6.48",
    "chokidar": "^3.5.2",
    "concurrently": "^6.2.0",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "8.7.1",
    "fs-extra": "^10.0.0",
    "postcss-import": "11.0.0",
    "postcss-loader": "^5.2.0",
    "sass": "1.66.1",
    "sass-loader": "10.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "resolutions": {
    "**/minimatch": "3.1.2"
  }
}
